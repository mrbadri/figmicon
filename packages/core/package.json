{
  "name": "@figmicon/core",
  "version": "1.1.0",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "bin": {
    "figmicon": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@figma/rest-api-spec": "^0.33.0",
    "c12": "^3.2.0",
    "commander": "^14.0.0",
    "kolorist": "^1.8.0",
    "p-limit": "^7.1.1",
    "zod": "^4.1.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
